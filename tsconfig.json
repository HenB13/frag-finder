{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es2021",
      "lib": ["es2021"],
      "module": "commonjs",
      "allowJs": true,
      "strictNullChecks": true,
      "resolveJsonModule": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "removeComments": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "baseUrl": "./"

    },
    "include": ["./src/**/*"]
  }